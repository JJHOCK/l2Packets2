$(TARGET): $(PCHC) $(OBJECTS)
	echo [LIB] $(TARGET)
	$(LIB) $(LIBFLAGS) $(TARGET) $(OBJECTS)

$(PCHC): $(SRC)/$(PCH).h
	echo [PCH] $(SRC)/$(PCH).h
	$(CREATE_PCH) $(SRC)/$(PCH).h -o $(PCHC)

$(BUILDDIR)/ByteArray.o: $(BASEDEPS) $(SRC)/base/ByteArray.cpp
	echo [C++] $(SRC)/base/ByteArray.cpp
	$(COMPILE) $(SRC)/base/ByteArray.cpp -o $(BUILDDIR)/ByteArray.o

